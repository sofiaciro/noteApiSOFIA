<html lang="en">

<head>
	<%- include('../partials/head'); %>
</head>

<body class="container">
	<%- include('../partials/body'); %>
		<div >
			<% listadoProductos.forEach(producto=> { %>
				<div class="card" style="width: 18rem;">
					<div class="card-body">
						<h3>
							<%= producto.nombre %>
						</h3>
						<h5>precio : <%= producto.precio %></h5>
						<p class="card-text">
							<%= producto.descripcion %>
						</p>
					</div>
				</div>
				<%})%>
		</div>
		<%- include ('../partials/footer'); %>
</body>